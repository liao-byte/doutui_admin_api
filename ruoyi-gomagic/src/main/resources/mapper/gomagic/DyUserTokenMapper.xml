<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.gomagic.mapper.DyUserTokenMapper">

    <select id="getByDyNumber" resultType="com.ruoyi.gomagic.domain.DyUserToken">
        SELECT * FROM dy_user_token WHERE dy_number = #{dyNumber} Order by create_time desc LIMIT 1
    </select>
</mapper>
